{
  "name": "fw2/glimpse",
  "type": "library",
  "license": "MIT",
  "require": {
    "phpstan/phpdoc-parser": "2.1.0",
    "nikic/php-parser": "^5.5"
  },
  "autoload": {
    "psr-4": {
      "Fw2\\Glimpse\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "symfony/var-dumper": "^7.3",
    "phpstan/phpstan": "^2.1",
    "pestphp/pest": "^3.8",
    "mockery/mockery": "^1.6",
    "squizlabs/php_codesniffer": "3.*"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
